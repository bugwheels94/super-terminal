{
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/npm",
		[
			"@semantic-release/exec",
			{
				"publishCmd": "find mac-out -type f && ind ubuntu-out -type f && ls"
			}
		],
		[
			"@semantic-release/exec",
			{
				"publishCmd": "mv \"mac-out/out/make/zip/darwin/x64/Super Terminal-darwin-x64-1.0.0.zip\" \"mac-out/Super Terminal-darwin-x64-${nextRelease.version}.zip\" && mv \"mac-out/Super Terminal.dmg\" \"mac-out/Super Terminal-${nextRelease.version}.dmg\" && mv \"ubuntu-out/zip/linux/x64/Super Terminal-linux-x64-1.0.0.zip\" \"ubuntu-out/Super Terminal-linux-x64-${nextRelease.version}.zip\" && mv \"ubuntu-out/rpm/x64/super-terminal-1.0.0-1.x86_64.rpm\" \"ubuntu-out/super-terminal-${nextRelease.version}-1.x86_64.rpm\" && mv \"ubuntu-out/deb/x64/super-terminal_1.0.0-_amd64.deb\" \"ubuntu-out/super-terminal_${nextRelease.version}_amd64.deb\""
			}
		],
		[
			"@semantic-release/github",
			{
				"assets": [
					{
						"path": "mac-out/*.zip"
					},
					{
						"path": "windows-out/*.zip"
					},
					{
						"path": "ubuntu-out/*.zip"
					},
					{
						"path": "windows-out/*.exe"
					},
					{
						"path": "ubuntu-out/*.rpm"
					},
					{
						"path": "ubuntu-out/*.deb"
					}
				]
			}
		]
	]
}
