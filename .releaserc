{
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/npm",
		[
			"@semantic-release/exec",
			{
				"publishCmd": "find mac-out -type f && find ubuntu-out -type f && ls"
			}
		],
		[
			"@semantic-release/exec",
			{
				"publishCmd": "mkdir tmp-assets && mv \"mac-out/out/make/zip/darwin/x64/Super Terminal-darwin-x64-1.0.0.zip\" \"tmp-assets/Super Terminal-darwin-x64-${nextRelease.version}.zip\" && mv \"mac-out/Super Terminal.dmg\" \"tmp-assets/Super Terminal-${nextRelease.version}.dmg\" && mv \"ubuntu-out/zip/linux/x64/Super Terminal-linux-x64-1.0.0.zip\" \"tmp-assets/Super Terminal-linux-x64-${nextRelease.version}.zip\" && mv \"ubuntu-out/rpm/x64/super-terminal-1.0.0-1.x86_64.rpm\" \"tmp-assets/super-terminal-${nextRelease.version}-1.x86_64.rpm\" && mv \"ubuntu-out/deb/x64/super-terminal_1.0.0_amd64.deb\" \"tmp-assets/super-terminal_${nextRelease.version}_amd64.deb\""
			}
		],
		[
			"@semantic-release/github",
			{
				"assets": [
					{
						"path": "my-assets/*"
					}
				]
			}
		]
	]
}
