{
	"name": "super-terminal",
	"version": "0.0.0-development",
	"description": "Terminal Manager",
	"repository": "https://github.com/super-terminal/super-terminal",
	"license": "MIT",
	"keywords": [],
	"main": "dist/index.js",
	"bin": {
		"super-terminal": "dist/index.js"
	},
	"scripts": {
		"build": "rollup -c",
		"test": "run-s build test:*",
		"start:watch": "ts-node src/index.ts",
		"typeorm": "typeorm-ts-node-commonjs",
		"prepare": "husky install"
	},
	"engines": {
		"node": ">=10"
	},
	"devDependencies": {
		"@babel/core": "^7.18.0",
		"@babel/plugin-proposal-class-properties": "^7.17.12",
		"@babel/plugin-proposal-decorators": "^7.17.9",
		"@babel/plugin-transform-regenerator": "^7.18.0",
		"@babel/plugin-transform-runtime": "^7.18.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.2.1",
		"@semantic-release/github": "^8.0.4",
		"@semantic-release/npm": "^9.0.1",
		"@types/js-yaml": "^4.0.5",
		"@types/lodash": "^4.14.182",
		"@types/node": "^16.11.10",
		"@types/sqlite3": "^3.1.8",
		"babel-plugin-const-enum": "^1.2.0",
		"fast-glob": "^3.2.11",
		"husky": "^8.0.1",
		"reflect-metadata": "^0.1.13",
		"rollup": "^2.70.2",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"semantic-release": "^19.0.2",
		"ts-node": "10.7.0",
		"typescript": "4.5.2",
		"@lytejs/ws": "0.0.0-development"
	},
	"pkg": {
		"assets": ["node_modules/super-terminal-ui/dist", "node_modules/node-pty", "node_modules/sqlite3", "../node_modules/node-pty", "../node_modules/sqlite3"],
      "targets": ["node16-macos-x64"]
	},
	"files": [
		"dist",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@babel/runtime": "^7.17.9",
		"babel-plugin-transform-typescript-metadata": "^0.3.2",
		"express": "^4.18.1",
		"js-yaml": "^4.1.0",
		"lodash": "^4.17.21",
		"node-pty": "^0.10.1",
		"sqlite3": "^4.0.3",
		"typeorm": "0.3.6",
		"ws": "^8.5.0"
	},
	"prettier": {
		"singleQuote": true,
		"printWidth": 120,
		"useTabs": true
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	}
}